#Requires AutoHotkey v2.0
#SingleInstance Force

NumLock::Send "{Volume_Mute}"
NumpadDiv::Send "{Media_Play_Pause}"
NumpadMult::Send "{Media_Prev}"
NumpadSub::Send "{Media_Next}"
Numpad4::Send "^+D"
Numpad5::Send "^+O"
Numpad6::Send "!5"
Numpad7::Send "^+M"
Numpad8::Send "{Volume_Down}"
Numpad9::Send "{Volume_Up}"
